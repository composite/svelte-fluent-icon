{
	"name": "svelte-fluent-icon",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "pnpm run clean && pnpm run generate && svelte-kit sync && svelte-package",
		"prepack:origin": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"generate": "pnpm run clean:gen && pnpm dlx tsx ./generate-icon.ts",
		"clean": "rm -rf dist",
		"clean:gen": "rm -rf src/lib/icon/1? && rm -rf src/lib/icon/2? && rm -rf src/lib/icon/32 && rm -rf src/lib/icon/48",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/icon/index.js",
	"types": "./dist/icon/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/icon/index.d.ts",
			"svelte": "./dist/icon/index.js",
			"default": "./dist/icon/index.js"
		},
		"./icon/*": {
			"types": "./dist/icon/*/index.d.ts",
			"svelte": "./dist/icon/*/index.js",
			"default": "./dist/icon/*/index.js"
		},
		"./icon/*.svelte": {
			"types": "./dist/icon/*.svelte.d.ts",
			"svelte": "./dist/icon/*.svelte"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@iconify-json/fluent": "^1.2.33",
		"@iconify/utils": "^3.0.2",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/node": "^20",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"playwright": "^1.55.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.13",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^1.1.0"
	},
	"keywords": [
		"svelte",
    "fluent",
    "icons",
    "fluent icons",
    "svelte-fluent-icons"
	]
}
